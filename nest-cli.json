{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "packages/webrtc-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "packages/webrtc-server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "packages/webrtc-server",
  "projects": {
    "webrtc-server": {
      "type": "application",
      "root": "packages/webrtc-server",
      "entryFile": "main",
      "sourceRoot": "packages/webrtc-server/src",
      "compilerOptions": {
        "tsConfigPath": "packages/webrtc-server/tsconfig.app.json"
      }
    },
    "loadbalancer": {
      "type": "application",
      "root": "packages/loadbalancer",
      "entryFile": "main",
      "sourceRoot": "packages/loadbalancer/src",
      "compilerOptions": {
        "tsConfigPath": "packages/loadbalancer/tsconfig.app.json"
      }
    }
  }
}
